{
  "name": "DavidPassportCard",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start --reset-cache",
    "test": "jest",
    "update": "yarn; cd ios && pod cache clean --all && pod install",
    "cleanIos": "cd ios && rm -rf Pods Podfile.lock build && pod install",
    "removeNodeModules": "rm -rf package-lock.json  node_modules yarn.lock /tmp/metro-*",
    "reset": "yarn clean && yarn removeNodeModules && yarn update",
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && npm cache clean --force && yarn cache clean"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "4",
    "@react-native-async-storage/async-storage": "^1.19.1",
    "@react-native-masked-view/masked-view": "^0.2.9",
